<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Must_Have_City_and_State</fullName>
    <active>true</active>
    <errorConditionFormula>AND(OR(ISBLANK(BillingCity), ISBLANK(BillingState)))</errorConditionFormula>
    <errorDisplayField>BillingCity</errorDisplayField>
    <errorMessage>Must insert a billing city and state</errorMessage>
</ValidationRule>
